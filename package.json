{
  "name": "yourtableredo",
  "version": "2.0.0",
  "description": "Check-in web app",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "dev": "env-cmd -f ./config/dev.env nodemon src/app.js -e js,ejs",
    "wizard": "workbox wizard --injectManifest",
    "generate-sw": "workbox injectManifest",
    "start-dev": "npm-run-all --parallel dev watch:sass",
    "start": "node src/app.js"
  },
  "keywords": [
    "progressive",
    "web",
    "app",
    "live"
  ],
  "author": "Dinh Vu",
  "license": "ISC",

    "body-parser": "^1.19.0",
    "concat": "^1.0.3",
    "connect-flash": "^0.1.1",
    "ejs": "^2.7.1",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.8",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
